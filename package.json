{
    "name": "breakout-area",
    "version": "1.0.0",
    "workspaces": [
        "client",
        "server"
    ],
    "scripts": {
        "dev": "concurrently \"npm run dev --workspace=breakout-area-server\" \"npm run dev --workspace=breakout-area-client\"",
        "seed": "npm run seed --workspace=breakout-area-server",
        "build": "npm run build --workspace=breakout-area-client",
        "start": "npm start --workspace=breakout-area-server"
    },
    "devDependencies": {
        "concurrently": "^8.2.2"
    }
}