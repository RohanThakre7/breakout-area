{
    "version": 2,
    "functions": {
        "server/server.js": {
            "runtime": "@vercel/node"
        }
    },
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/server/server.js"
        },
        {
            "src": "/uploads/(.*)",
            "dest": "/server/server.js"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}